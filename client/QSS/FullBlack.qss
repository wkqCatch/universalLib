    color: #454545;
}

QPushButton:focus 
{
    border:1px solid #3daee9;
    color: white;
}

QPushButton:hover
{
	border: 1px solid #3daee9;
    color: #eff0f1;
	background-color: rgba(200,200,200,100);
}

QPushButton:pressed
{
    background-color: #3daee9;
    padding-top: -15px;
